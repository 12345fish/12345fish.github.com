<script src="js/tvp.player_v2_jq.js"></script>
<script>
$(function(){
    var $cover = $('#cover'),
        $videoBg = $('#videoBg'),
        $tPlayer = null;
    // 取得页头、页脚
    var toolbarH = 0,
        footerH = 0;
    // 封面图片原始尺寸、窗口大小,封面容器、logo尺寸，标题字号
    var imgW, imgH, winW, winH, coverW, coverH;
    // 封面、图片、视频比例
    var coverRatio, imgRatio, hRatiov, vRatio = 16 / 9;
    // 视频缩小
    var videoCutW = 300;
    //是否加载过图片
    var imgReady = false;
    var videoSupport = checkVideoSupport();
    var resizing = null;
    var tPlayerAdded = false;
    // 初始化
    function init() {
        var _speed = 500;
        coverInit();
        playBtn();
        $(window).resize(function() {
            if (resizing) {
                clearTimeout(resizing);
            }
            resizing = setTimeout(function() {
                coverInit();
            }, 30);
        });
    }
    // 播放按钮事件
    function playBtn(){
        $(".playBtn").each(function(i, obj){
            $(obj).click(function(event) {
                createTplayer($(this).attr('data-vid'), 'tplayer');
                event.preventDefault();
            });
        });
    };
    // 封面初始化
    function coverInit() {
        // 窗口尺寸
        winW = $(window).width(),
        winH = $(window).height();
        // cover高度
        coverW = winW,
        coverH = winH - toolbarH - footerH;
        coverW2 = winW - videoCutW;
        if(!$cover.is('.thanks')){
            $cover.css({ height: coverH + 'px' });
        } else {
            $cover.css({ height: (coverW/imgRatio) + 'px' });
        }
        // cover比例
        coverRatio = coverW / coverH;
        hRatio = coverH / 830;
        // 主视频缩放
        if (tPlayerAdded) {
            scaleMainV();
        }
    }
    // 加载图片
    function imgLoad(url, callback) {
    }
    // 检测浏览是否支持视频
    function checkVideoSupport() {
        var tempVideo = document.createElement('video');
        if (tempVideo.canPlayType && !$cover.is('.thanks')) {
            $videoBg.on('canplay',function(){
                this.play();
            });
            return true;
        } else {
            //imgLoad($imgBg.attr('src'), function() {
            //});
            return false;
        }
    }
    // 切换背景视频
    function changeVideo(_, _src) {
        if (!_) return;
        _.attr("src", _src);
    };
    // 创建首屏主视频播放播放框
    function createTplayer(vid,targetId) {
        if (!tPlayerAdded) {
            var tempV = $('<div class="V"><div id="mainvideo" class="mainvideo"><div class="close"  id="close"></div><div class="tplayer" id="tplayer"></div></div></div>');
            $videoBg.after(tempV);
            $tPlayer = $('#tplayer');
            scaleMainV();
            // 缩放腾讯视频播放器窗口
            addTenPlayer(vid,targetId,'100%','100%',function(){
                doScroll('down');
                removeTenPlayer();
            });
            tPlayerAdded = true;
            $('#close').click(removeTenPlayer);
            $("#tplayer").click(function(e){
                e.stopPropagation();
            });
            $("#mainvideo").click(function(){
                $('#close').click();
            });
        }
    }
    // 缩放首屏主视频播放框
    function scaleMainV() {
        var tempW, tempH, tempLeft, tempTop;
        coverRatio = coverW2 / coverH;
        if (vRatio <= coverRatio) {
            tempH = coverH;
            tempW = (tempH -20)* vRatio; //腾讯视频播放器皮肤有20的边
        } else {
            tempW = coverW2;
            tempH = (coverW2 / vRatio)+20;
        }
        tempLeft = (coverW2 - tempW) / 2 + videoCutW / 2;
        tempTop = (coverH - tempH) / 2;
        $tPlayer.css({
            width: '960px',
            height:'600px'
        })
    }
    // 添加腾讯视频播放器
    function addTenPlayer(vid, place, w, h,callback) {
        var video = new tvp.VideoInfo();
        video.setVid(vid);
        var myplayer = new tvp.Player();
        myplayer.create({
            width: w || 100 + '%',
            height: h || 100 + '%',
            video: video,
            modId: place,
            autoplay:true,
            vodFlashSkin: "http://imgcache.qq.com/minivideo_v1/vd/res/skins/TencentPlayerMiniSkin.swf",
            flashWmode: 'opaque',
            onallended:callback
        });
    }
    // 移除首屏主视频
    function removeTenPlayer() {
        if (tPlayerAdded) {
            $('.mainvideo,.V').remove();
            tPlayerAdded = false;
        }
    }
    init();
});
</script>
